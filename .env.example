# Environment variables for Agno NFT Creator Agent
# Rename this file to .env and fill in your actual values

# Supabase Configuration
SUPABASE_URL=your_supabase_url
SUPABASE_KEY=your_supabase_key
# SUPABASE_CONNECTION_STRING="YOUR_SUPABASE_DB_CONNECTION_STRING_WITH_PASSWORD" # Optional, if needed for direct DB access tools

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key

# BRAINTRUST Configuration
BRAINTRUST_API_KEY=your_braintrust_api_key

# NMKR Configuration
NMKR_API_KEY=your_nmkr_api_key
NMKR_PROJECT_UID=your_nmkr_project_uid
NMKR_POLICY_ID=your_nmkr_policy_id

# Masumi Configuration
MASUMI_API_KEY=your_masumi_api_key
MASUMI_PAYMENT_URL=http://host.docker.internal:3001

# Replicate Configuration
REPLICATE_API_TOKEN=your_replicate_api_token

# Receive Wallet Address
RECEIVE_WALLET=your_cardano_wallet_address

# RabbitMQ Configuration
RABBITMQ_URL=amqp://guest:guest@rabbitmq:5672/

# Telegram Bot Configuration
TELEGRAM_BOT_TOKEN=your_telegram_bot_token

# Hot Wallet Configuration (Optional - For demo signing, handle securely)
HOT_WALLET_MNEMONIC="YOUR_CARDANO_HOT_WALLET_SEED_PHRASE" # Store securely!

# SERVICES
API_URL=http://agent-api:8000

# PAYMENT SETTINGS
PAYMENT_REDIRECT_URL=https://example.com/payment/success
PAYMENT_CALLBACK_URL=https://example.com/payment/callback

# Optional Env Variables
# AGENT SETTINGS
LOG_LEVEL=INFO 